{
  "name": "growlerapp",
  "version": "3.0.94",
  "description": "App for search your near growler",
  "main": "src",
  "scripts": {
    "start": "node src",
    "dev": "nodemon --inspect=0.0.0.0:9229 src",
    "dev:docker": "nodemon --inspect=0.0.0.0:9229 src",
    "initdb": "node src/initdb",
    "lint": "eslint .",
    "format": "prettier-standard 'src/**/*.js' 'test/**/*.js'",
    "test": "MONGODB_URI=mongodb://localhost/growler_test nyc mocha",
    "ts-compile-check": "tsc -p tsconfig.json --noEmit"
  },
  "engines": {
    "node": "12.13.1"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/growlerapp/api.git"
  },
  "keywords": [
    "growler",
    "graphql"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/growlerapp/api/issues"
  },
  "homepage": "https://github.com/growlerapp/api#readme",
  "dependencies": {
    "@godaddy/terminus": "4.2.1",
    "@google/maps": "0.5.5",
    "@sentry/node": "5.9.0",
    "apollo-server-express": "2.9.12",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "graphql": "14.5.8",
    "graphql-geojson": "1.0.0",
    "helmet": "3.21.2",
    "mongoose": "5.7.12",
    "morgan": "1.9.1",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@eclass/eslint-config": "1.2.1",
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/commit-analyzer": "6.3.3",
    "@semantic-release/exec": "3.3.8",
    "@semantic-release/git": "7.0.18",
    "@semantic-release/github": "5.5.5",
    "@semantic-release/npm": "5.3.4",
    "@semantic-release/release-notes-generator": "7.3.4",
    "@types/body-parser": "1.17.1",
    "@types/cors": "2.8.6",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.2",
    "@types/google__maps": "0.5.8",
    "@types/graphql": "14.2.3",
    "@types/helmet": "0.0.45",
    "@types/mongoose": "5.5.32",
    "@types/morgan": "1.7.37",
    "@types/node": "12.12.11",
    "chai": "4.2.0",
    "eslint": "6.7.1",
    "eslint-plugin-array-func": "3.1.3",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsdoc": "18.1.5",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sonarjs": "0.5.0",
    "eslint-plugin-standard": "4.0.1",
    "husky": "3.1.0",
    "lint-staged": "9.4.3",
    "mocha": "6.2.2",
    "nodemon": "2.0.1",
    "nyc": "14.1.1",
    "nyc-config-common": "1.0.1",
    "prettier-standard": "15.0.1",
    "semantic-release": "15.13.31",
    "supertest": "4.0.2",
    "typescript": "3.6.4"
  },
  "eslintConfig": {
    "extends": "@eclass"
  },
  "eslintIgnore": [
    "coverage"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "eslint --fix",
      "prettier-standard",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "renovate": {
    "extends": [
      "config:js-app"
    ],
    "minor": {
      "automerge": true
    },
    "major": {
      "automerge": false
    },
    "assignees": [
      "lgatica"
    ],
    "reviewers": [
      "lgaticaq"
    ],
    "packageRules": [
      {
        "packageNames": [
          "typescript"
        ],
        "allowedVersions": "<3.7"
      }
    ]
  },
  "mocha": {
    "exit": true
  },
  "nyc": {
    "extends": "nyc-config-common"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github",
      [
        "@semantic-release/exec",
        {
          "verifyConditionsCmd": "bash .travis/heroku.container-release-verify.sh",
          "publishCmd": "bash .travis/heroku.container-release-publish.sh"
        }
      ]
    ]
  }
}
